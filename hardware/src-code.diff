+    if (rst) begin
+        write_start_index_reg <= 0;
+        read_start_index_reg <= 0;
+
+        for (queue_no=0; queue_no<NUM_FIFOS; queue_no=queue_no+1) begin
+            // write part
+            fifo_in_tdata_reg[queue_no] <= 0;
+            fifo_in_tvalid_reg[queue_no] <= 0;
