+        m_output_tdata_reg <= 0;
+        m_output_tvalid_reg <= 0;
+        m_available_bytes_reg <= 0;
+
+        m_output_tready_d1 <= 0;
+        has_enough_bytes_d1 <= 0;
+        m_consumed_bytes_d1 <= 0;
+
+        rd_wrap_len_index_wide_reg <= 0;
+        rd_wrap_len_index_end_reg <= 0;
