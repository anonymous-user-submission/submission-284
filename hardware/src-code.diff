+    for (queue_no=0; queue_no<NUM_FIFOS; queue_no=queue_no+1) begin
+        // write part
+        fifo_in_tdata_reg[queue_no] <= fifo_in_tdata_next[queue_no];
+        fifo_in_tvalid_reg[queue_no] <= fifo_in_tvalid_next[queue_no];
