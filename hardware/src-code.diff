+reg [MSG_ID_WIDTH-1:0]                  m_egress_to_retx_msg_id_reg = 0, m_egress_to_retx_msg_id_next;
+reg [MSG_OFF_WIDTH-1:0]                 m_egress_to_retx_msg_offset_reg = 0, m_egress_to_retx_msg_offset_next;
+reg [CL_RETX_DESC_TABLE_SIZE-1:0]       m_egress_to_retx_table_ptr_reg = 0, m_egress_to_retx_table_ptr_next;
+reg                                     m_egress_to_retx_msg_valid_reg = 0, m_egress_to_retx_msg_valid_next;
+reg                                     m_egress_to_retx_is_msg_reg = 0, m_egress_to_retx_is_msg_next;
+
+wire [TAG_WIDTH-1:0]                    retx_table_ptr;
+
+
+assign m_egress_to_retx_msg_id      = m_egress_to_retx_msg_id_reg;
+assign m_egress_to_retx_msg_offset  = m_egress_to_retx_msg_offset_reg;
+assign m_egress_to_retx_table_ptr   = m_egress_to_retx_table_ptr_reg;
+assign m_egress_to_retx_msg_valid   = m_egress_to_retx_msg_valid_reg;
+assign m_egress_to_retx_is_msg      = m_egress_to_retx_is_msg_reg;
+
+// output
+
+wire [AXIS_DATA_WIDTH-1:0] in_data[1:0];
+wire [AXIS_KEEP_WIDTH-1:0] in_keep[1:0];
+wire in_valid [1:0];
+wire in_ready [1:0];
+wire in_last [1:0];
+wire [AXIS_ID_WIDTH-1:0] in_tid[1:0];
+wire [AXIS_DEST_WIDTH-1:0] in_dest[1:0];
+wire [AXIS_USER_WIDTH-1:0] in_user[1:0];
+
+wire [AXIS_DATA_WIDTH-1:0] fifo_out_data[1:0];
+wire [AXIS_KEEP_WIDTH-1:0] fifo_out_keep[1:0];
+wire fifo_out_valid [1:0];
+reg  fifo_out_ready [1:0];
+wire fifo_out_last [1:0];
+wire [AXIS_ID_WIDTH-1:0] fifo_out_tid[1:0];
+wire [AXIS_DEST_WIDTH-1:0] fifo_out_dest[1:0];
+wire [AXIS_USER_WIDTH-1:0] fifo_out_user[1:0];
+
+assign in_data[0]   = m_axis_tdata_int;
+assign in_keep[0]   = m_axis_tkeep_int;
+assign in_tid[0]    = m_axis_tid_int;
+assign in_user[0]   = m_axis_tuser_int;
+assign in_dest[0]   = m_axis_tdest_int;
+assign in_valid[0]  = m_axis_tvalid_int;
+assign in_last[0]   = m_axis_tlast_int;
+assign m_axis_tready_int = in_ready[0];
+
+// assign in_data[1]   = s_axis_ack_tdata;
+// assign in_keep[1]   = s_axis_ack_tkeep;
+// assign in_tid[1]    = s_axis_ack_tid;
+// assign in_user[1]   = s_axis_ack_tuser;
+// assign in_dest[1]   = s_axis_ack_tdest;
+// assign in_valid[1]  = s_axis_ack_tvalid;
+// assign in_last[1]   = s_axis_ack_tlast;
+// assign s_axis_ack_tready = in_ready[1];
+assign in_data[1] = 0;
+assign in_keep[1] = 0;
+assign in_tid[1] = 0;
+assign in_user[1] = 0;
+assign in_dest[1] = 0;
+assign in_valid[1] = 0;
+assign in_last[1] = 0;
+
+localparam NUM_STATES_BITS = 4;
+reg cur_q, cur_q_next;
+wire cur_q_plus1;
+
+// assign cur_q_plus1 = (cur_q == 1) ? 0 : cur_q+1;
+assign cur_q_plus1 = 0;
+
+reg [NUM_STATES_BITS-1:0] data_out_state, data_out_state_next;
+
+wire [TAG_WIDTH-1:0] tx_tag_fifo_out;
+wire tx_tag_fifo_out_valid;
+reg  tx_tag_fifo_out_ready;
+axis_fifo #(
+    .DEPTH(64),
+    .DATA_WIDTH(TAG_WIDTH),
+    .KEEP_ENABLE(0),
+    .LAST_ENABLE(0),
+    .ID_ENABLE(0),
+    .DEST_ENABLE(0),
+    .USER_ENABLE(0),
+    .FRAME_FIFO(0)
+)
+tx_tag_fifo (
+    .clk            (clk),
+    .rst            (rst),
+
+    // input
+    .s_axis_tdata           (s_axis_tx_desc_status_tag),
+    .s_axis_tkeep           (),
+    .s_axis_tvalid          (s_axis_tx_desc_status_valid),
+    .s_axis_tready          (),
+    .s_axis_tlast           (),
+    .s_axis_tid             (),
+    .s_axis_tdest           (),
+    .s_axis_tuser           (),
+    // output
+    .m_axis_tdata           (tx_tag_fifo_out),
+    .m_axis_tkeep           (),
+    .m_axis_tvalid          (tx_tag_fifo_out_valid),
+    .m_axis_tready          (tx_tag_fifo_out_ready),
+    .m_axis_tlast           (),
+    .m_axis_tid             (),
+    .m_axis_tdest           (),
+    .m_axis_tuser           (),
+    // status
+    .status_overflow        (),
+    .status_bad_frame       (),
+    .status_good_frame      ()
+);
+
+assign retx_table_ptr = {{CL_DESC_TABLE_SIZE{1'b0}}, (tx_tag_fifo_out>>CL_DESC_TABLE_SIZE)}
+                                                                                & {CL_RETX_DESC_TABLE_SIZE{1'b1}};
+
+generate
+    genvar i;
+    for (i=0; i<2; i=i+1) begin: input_data
+        axis_fifo #(
+            .DEPTH(64*AXIS_DATA_WIDTH),
+            .DATA_WIDTH(AXIS_DATA_WIDTH),
+            .KEEP_WIDTH(AXIS_KEEP_WIDTH),
+            .KEEP_ENABLE(1),
+            .LAST_ENABLE(1),
+            .ID_ENABLE(1),
+            .ID_WIDTH(AXIS_ID_WIDTH),
+            .DEST_ENABLE(1),
+            .DEST_WIDTH(AXIS_DEST_WIDTH),
+            .USER_ENABLE(1),
+            .USER_WIDTH(AXIS_USER_WIDTH),
+            .FRAME_FIFO(0)
+        )
+        data_fifo (
+            .clk            (clk),
+            .rst            (rst),
+            // input
+            .s_axis_tdata           (in_data[i]),
+            .s_axis_tkeep           (in_keep[i]),
+            .s_axis_tvalid          (in_valid[i] & in_ready[i]),
+            .s_axis_tready          (in_ready[i]),
+            .s_axis_tlast           (in_last[i]),
+            .s_axis_tid             (in_tid[i]),
+            .s_axis_tdest           (in_dest[i]),
+            .s_axis_tuser           (in_user[i]),
+            // output
+            .m_axis_tdata           (fifo_out_data[i]),
+            .m_axis_tkeep           (fifo_out_keep[i]),
+            .m_axis_tvalid          (fifo_out_valid[i]),
+            .m_axis_tready          (fifo_out_ready[i]),
+            .m_axis_tlast           (fifo_out_last[i]),
+            .m_axis_tid             (fifo_out_tid[i]),
+            .m_axis_tdest           (fifo_out_dest[i]),
+            .m_axis_tuser           (fifo_out_user[i]),
+            // status
+            .status_overflow        (),
+            .status_bad_frame       (),
+            .status_good_frame      ()
+        );
+    end
+
+endgenerate
+
+reg  [AXIS_DATA_WIDTH-1:0]  m_axis_tdata_reg;
+reg  [AXIS_KEEP_WIDTH-1:0]  m_axis_tkeep_reg;
+reg                         m_axis_tvalid_reg;
+reg                         m_axis_tlast_reg;
+reg  [AXIS_ID_WIDTH-1:0]    m_axis_tid_reg;
+reg  [AXIS_DEST_WIDTH-1:0]  m_axis_tdest_reg;
+reg  [AXIS_USER_WIDTH-1:0]  m_axis_tuser_reg;
+
+reg  [AXIS_DATA_WIDTH-1:0]  m_axis_tdata_next;
+reg  [AXIS_KEEP_WIDTH-1:0]  m_axis_tkeep_next;
+reg                         m_axis_tvalid_next;
+reg                         m_axis_tlast_next;
+reg  [AXIS_ID_WIDTH-1:0]    m_axis_tid_next;
+reg  [AXIS_DEST_WIDTH-1:0]  m_axis_tdest_next;
+reg  [AXIS_USER_WIDTH-1:0]  m_axis_tuser_next;
+
+assign m_axis_tdata     = m_axis_tdata_reg;
+assign m_axis_tkeep     = m_axis_tkeep_reg;
+assign m_axis_tuser     = m_axis_tuser_reg;
+assign m_axis_tid       = m_axis_tid_reg;
+assign m_axis_tdest     = m_axis_tdest_reg;
+assign m_axis_tvalid    = m_axis_tvalid_reg;
+assign m_axis_tlast     = m_axis_tlast_reg;
+
+wire [AXIS_DATA_WIDTH-1:0]  dbg_axis_tdata = fifo_out_data[0];
+wire [AXIS_KEEP_WIDTH-1:0]  dbg_axis_tkeep = fifo_out_keep[0];
+wire                        dbg_axis_tvalid = fifo_out_valid[0];
+wire                        dbg_axis_tlast = fifo_out_last[0];
+wire [AXIS_ID_WIDTH-1:0]    dbg_axis_tid = fifo_out_tid[0];
+wire [AXIS_DEST_WIDTH-1:0]  dbg_axis_tdest = fifo_out_dest[0];
+
+localparam WAIT_FIRST_PKT_SEG = 0;
+localparam FLUSH = 1;
+
+integer j;
+
+always @(*) begin
+    data_out_state_next = data_out_state;
+    cur_q_next = cur_q;
+
+    tx_tag_fifo_out_ready = 1'b0;
+    for (j=0; j<2; j=j+1) begin
+        fifo_out_ready[j] = 0;
+    end
+
+    // output data
+    m_axis_tdata_next       = m_axis_tdata_reg;
+    m_axis_tkeep_next       = m_axis_tkeep_reg;
+    m_axis_tvalid_next      = m_axis_tvalid_reg && !m_axis_tready;
+    m_axis_tlast_next       = 0;
+    m_axis_tid_next         = m_axis_tid_reg;
+    m_axis_tdest_next       = m_axis_tdest_reg;
+    m_axis_tuser_next       = m_axis_tuser_reg;
+
+    // egress-to-retx output
+    m_egress_to_retx_msg_id_next      =   m_egress_to_retx_msg_id_reg;
+    m_egress_to_retx_msg_offset_next  =   m_egress_to_retx_msg_offset_reg;
+    m_egress_to_retx_is_msg_next      =   m_egress_to_retx_is_msg_reg;
+    // following 2 should be set at tlast
+    m_egress_to_retx_table_ptr_next   =   m_egress_to_retx_table_ptr_reg;
+    m_egress_to_retx_msg_valid_next   =   m_egress_to_retx_msg_valid_reg && !m_egress_to_retx_msg_ready;
+
+    case (data_out_state)
+        WAIT_FIRST_PKT_SEG: begin
+            if (fifo_out_valid[cur_q]) begin
+                // from data fifo
+                if (cur_q == 0) begin
+                    if (fifo_out_data[cur_q][12*8 +: 16] == IP_ETHTYPE
+                            && fifo_out_data[cur_q][23*8 +: 8] == UDP_IPPROTO
+                            && fifo_out_data[cur_q][36*8 +: 16] == MSG_DST_PORT) begin
+                        // MSG packet
+                        m_egress_to_retx_msg_id_next = {fifo_out_data[cur_q][59*8 +:8],
+                                                            fifo_out_data[cur_q][58*8 +: 8],
+                                                            fifo_out_data[cur_q][57*8 +: 8],
+                                                            fifo_out_data[cur_q][56*8 +: 8]};
+                        m_egress_to_retx_msg_offset_next = fifo_out_data[cur_q][44*8 +: 8];
+                        m_egress_to_retx_is_msg_next = 1'b1;
+                    end
+                    else begin
+                        m_egress_to_retx_msg_id_next = 0;
+                        m_egress_to_retx_msg_offset_next = 0;
+                        m_egress_to_retx_is_msg_next = 1'b0;
+                    end
+                end
+                // from ack fifo
+                else begin
+                    m_egress_to_retx_msg_id_next = 0;
+                    m_egress_to_retx_msg_offset_next = 0;
+                    m_egress_to_retx_is_msg_next = 1'b0;
+                end
+
+                if (!m_axis_tvalid_reg || m_axis_tready) begin
+
+                    if (!fifo_out_last[cur_q]) begin
+                        fifo_out_ready[cur_q] = 1'b1;
+
+                        m_axis_tdata_next       = fifo_out_data[cur_q];
+                        m_axis_tkeep_next       = fifo_out_keep[cur_q];
+                        m_axis_tvalid_next      = 1'b1;
+                        m_axis_tlast_next       = fifo_out_last[cur_q];
+                        m_axis_tid_next         = fifo_out_tid[cur_q];
+                        m_axis_tdest_next       = fifo_out_dest[cur_q];
+                        m_axis_tuser_next       = fifo_out_user[cur_q];
+
+                        data_out_state_next = FLUSH;
+                    end
+                    // the last seg
+                    else begin
+                        m_axis_tdata_next       = fifo_out_data[cur_q];
+                        m_axis_tkeep_next       = fifo_out_keep[cur_q];
+                        m_axis_tlast_next       = fifo_out_last[cur_q];
+                        m_axis_tid_next         = fifo_out_tid[cur_q];
+                        m_axis_tdest_next       = fifo_out_dest[cur_q];
+                        m_axis_tuser_next       = fifo_out_user[cur_q];
+                        if (cur_q == 0) begin
+                            if (tx_tag_fifo_out_valid
+                            && (!m_egress_to_retx_msg_valid||m_egress_to_retx_msg_ready)) begin
+                                cur_q_next = cur_q_plus1;
+
+                                m_egress_to_retx_table_ptr_next = retx_table_ptr;
+                                m_egress_to_retx_msg_valid_next = 1'b1;
+
+                                fifo_out_ready[cur_q] = 1'b1;
+                                tx_tag_fifo_out_ready = 1'b1;
+                                m_axis_tvalid_next = 1'b1;
+                            end
+                        end
+                        else begin
+                            fifo_out_ready[cur_q] = 1'b1;
+                            m_axis_tvalid_next = 1'b1;
+                        end
+                    end
+                end
+            end
+            else begin
+                cur_q_next = cur_q_plus1;
+            end
+        end
+        FLUSH: begin
+            if (fifo_out_valid[cur_q]) begin
+                if (!m_axis_tvalid_reg || m_axis_tready) begin
+                    if (!fifo_out_last[cur_q]) begin
+                        fifo_out_ready[cur_q] = 1'b1;
+
+                        m_axis_tdata_next       = fifo_out_data[cur_q];
+                        m_axis_tkeep_next       = fifo_out_keep[cur_q];
+                        m_axis_tvalid_next      = 1'b1;
+                        m_axis_tlast_next       = fifo_out_last[cur_q];
+                        m_axis_tid_next         = fifo_out_tid[cur_q];
+                        m_axis_tdest_next       = fifo_out_dest[cur_q];
+                        m_axis_tuser_next       = fifo_out_user[cur_q];
+                    end
+                    else begin
+                        m_axis_tdata_next       = fifo_out_data[cur_q];
+                        m_axis_tkeep_next       = fifo_out_keep[cur_q];
+                        m_axis_tlast_next       = fifo_out_last[cur_q];
+                        m_axis_tid_next         = fifo_out_tid[cur_q];
+                        m_axis_tdest_next       = fifo_out_dest[cur_q];
+                        m_axis_tuser_next       = fifo_out_user[cur_q];
+
+                        if (cur_q == 0) begin
+                            if (tx_tag_fifo_out_valid
+                            && (!m_egress_to_retx_msg_valid||m_egress_to_retx_msg_ready)) begin
+                                cur_q_next = cur_q_plus1;
+
+                                m_egress_to_retx_table_ptr_next = retx_table_ptr;
+                                m_egress_to_retx_msg_valid_next = 1'b1;
+
+                                fifo_out_ready[cur_q] = 1'b1;
+                                tx_tag_fifo_out_ready = 1'b1;
+                                m_axis_tvalid_next = 1'b1;
+                                data_out_state_next = WAIT_FIRST_PKT_SEG;
+                            end
+                        end
+                        else begin
+                            fifo_out_ready[cur_q] = 1'b1;
+                            m_axis_tvalid_next = 1'b1;
+                            data_out_state_next = WAIT_FIRST_PKT_SEG;
+                            cur_q_next = cur_q_plus1;
+                        end
+                    end
+                end
+            end
+        end
+    endcase
+end
+
+always @(posedge clk) begin
+    if (rst) begin
+        data_out_state <= WAIT_FIRST_PKT_SEG;
+        cur_q <= 0;
+        // data output
+        m_axis_tdata_reg       <=   0;
+        m_axis_tkeep_reg       <=   0;
+        m_axis_tvalid_reg      <=   0;
+        m_axis_tlast_reg       <=   0;
+        m_axis_tid_reg         <=   0;
+        m_axis_tdest_reg       <=   0;
+        m_axis_tuser_reg       <=   0;
+        // egree-to-retx output
+        m_egress_to_retx_msg_id_reg      <= 0;
+        m_egress_to_retx_msg_offset_reg  <= 0;
+        m_egress_to_retx_table_ptr_reg   <= 0;
+        m_egress_to_retx_msg_valid_reg   <= 0;
+        m_egress_to_retx_is_msg_reg      <= 0;
+    end
+    else begin
+        data_out_state <= data_out_state_next;
+        cur_q <= cur_q_next;
+
+        m_axis_tdata_reg       <=   m_axis_tdata_next;
+        m_axis_tkeep_reg       <=   m_axis_tkeep_next;
+        m_axis_tvalid_reg      <=   m_axis_tvalid_next;
+        m_axis_tlast_reg       <=   m_axis_tlast_next;
+        m_axis_tid_reg         <=   m_axis_tid_next;
+        m_axis_tdest_reg       <=   m_axis_tdest_next;
+        m_axis_tuser_reg       <=   m_axis_tuser_next;
+
+        m_egress_to_retx_msg_id_reg      <= m_egress_to_retx_msg_id_next;
+        m_egress_to_retx_msg_offset_reg  <= m_egress_to_retx_msg_offset_next;
+        m_egress_to_retx_table_ptr_reg   <= m_egress_to_retx_table_ptr_next;
+        m_egress_to_retx_msg_valid_reg   <= m_egress_to_retx_msg_valid_next;
+        m_egress_to_retx_is_msg_reg      <= m_egress_to_retx_is_msg_next;
+    end
+end
+
 endmodule
 
