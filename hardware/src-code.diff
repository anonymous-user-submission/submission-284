+    // end
+
+    for (queue_no=0; queue_no<NUM_FIFOS; queue_no=queue_no+1) begin
+        // m_output_tdata_next[queue_no*8 +: 8] = remap_vec_tdata_reg[queue_no];
+        m_output_tdata_next[queue_no*8 +: 8] = remap_vec_tdata[queue_no];
+    end
