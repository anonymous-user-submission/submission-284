diff --color -burN ./base-rtl/tb/mqnic_core_pcie_us/cam_ram_conf.py ./rtl/tb/mqnic_core_pcie_us/cam_ram_conf.py
--- ./base-rtl/tb/mqnic_core_pcie_us/cam_ram_conf.py	1969-12-31 17:00:00.000000000 -0700
+++ ./rtl/tb/mqnic_core_pcie_us/cam_ram_conf.py	2023-12-06 10:50:23.234198291 -0700
@@ -0,0 +1,87 @@
+import scapy.utils
+from scapy.layers.l2 import Ether
+from scapy.layers.inet import IP, UDP
+
+import struct
+
+conf_cam_type = 0
+conf_ram_type = 1
+conf_eth_src_addr = "E0:E1:E2:E3:E4:E5"
+conf_eth_dst_addr = "EF:EF:EF:EF:EF:EF"
+conf_udp_dst_port = 0x1f1f
+conf_ip_src_addr = "25.25.25.25"
+conf_ip_dst_addr = "52.52.52.52"
+
+conf_st_ed_udp_dst_port = 0x2f2f
+conf_st_flag = 1
+conf_ed_flag = 2
+
+def st_ed_conf_pkt(if_start, app_id):
+    eth = Ether(src=conf_eth_src_addr, dst=conf_eth_dst_addr)
+    ip = IP(src=conf_ip_src_addr, dst=conf_ip_dst_addr)
+    udp = UDP(sport=1, dport=conf_st_ed_udp_dst_port)
+
+    if if_start == True:
+        conf_bytes = struct.pack(">BB", conf_st_flag, app_id)
+    else:
+        conf_bytes = struct.pack(">BB", conf_ed_flag, app_id)
+
+    raw_bytes = bytearray(20*b'\x00')
+
+    pkt = eth / ip / udp / conf_bytes / bytes(raw_bytes)
+    return pkt
+
+def conf_pkt(conf_bytes):
+    eth = Ether(src=conf_eth_src_addr, dst=conf_eth_dst_addr)
+    ip = IP(src=conf_ip_src_addr, dst=conf_ip_dst_addr)
+    udp = UDP(sport=1, dport=conf_udp_dst_port)
+
+    raw_bytes = bytearray(22*b'\x00')
+    raw_bytes.extend(conf_bytes)
+
+    pkt = eth / ip / udp / bytes(raw_bytes)
+    return pkt
+
+def get_conf(conf_filename):
+    conf_file = open(conf_filename, "r")
+    conf_pkts = []
+    line = conf_file.readline()
+    v = line.strip().split(" ")
+    app_id = int(v[0])
+    msg_type = int(v[1])
+    assert(msg_type == 1)
+    # start
+    conf_pkts.append(st_ed_conf_pkt(True, app_id))
+    while True:
+        line = conf_file.readline()
+        if not line:
+            break
+        v = line.strip().split(" ")
+
+        if v[0] == "RAM":
+            for i in range(int(v[1])):
+                line = conf_file.readline()
+                ram_conf = line.strip().split(" ")
+                # addr, field index bytes to skip, bytes of key, next state
+                conf_bytes = struct.pack(">BHBBBB", conf_ram_type,
+                        int(ram_conf[0]), int(ram_conf[1]), int(ram_conf[2]), int(ram_conf[3]), int(ram_conf[4]))
+                ram_conf_pkt = conf_pkt(conf_bytes)
+                conf_pkts.append(ram_conf_pkt)
+        elif v[0] == "CAM":
+            for i in range(int(v[1])):
+                line = conf_file.readline()
+                cam_conf = line.strip().split(" ")
+                # addr, app_id+msg_type, field_ind, cur_state, key to match
+                key = bytearray(cam_conf[4][::-1].encode("utf-8"))
+                key_len = len(key)
+                conf_bytes = struct.pack(">BHBBB%dx%ds" % ((24-key_len), key_len), conf_cam_type,
+                        int(cam_conf[0]), int(cam_conf[1]), int(cam_conf[2]), int(cam_conf[3]), key)
+                cam_conf_pkt = conf_pkt(conf_bytes)
+                conf_pkts.append(cam_conf_pkt)
+    # end
+    conf_pkts.append(st_ed_conf_pkt(False, app_id))
+    return conf_pkts
+
+if __name__ == "__main__":
+    pkts = get_conf()
+    scapy.utils.wrpcap("conf_pkts.pcap", pkts)
